void selectionSort(int arr[], int n)
	{
	    int min;
	    for(int i=0;i<n-1;i++)
	    {
	        min=i;
	        for(int j=i;j<n;j++)
	        {
	            if(arr[j]<arr[min])
	            {
	               min=j;
	            }
	        }
	        if(min!=i)
	        {
	       int temp=arr[i];
	        arr[i]=arr[min];
	        arr[min]=temp;
	        }
	        
	    }
}
